    'descriptionMoreURL': 'https://laravel.com/docs/master/blade#if-statements'
    'prefix': '@guest'
    'body': '''
              @guest
              \t$0
              @endguest
              '''
  '@error ':
    'description': 'Determines whether there is an input error associated with the field.'
    'descriptionMoreURL': 'https://github.com/laravel/framework/pull/28062#issue-265773495'
    'prefix': '@error'
    'body': '''
              @error ('${1:field}')
              \t$0
              @enderror
              '''
  '@production … @endproduction ':
    'description': 'Determines whether application is running in the production environment.'
    'descriptionMoreURL': 'https://laravel.com/docs/8.x/blade#environment-directives'
    'prefix': '@production'
    'body': '''
              @production
              \t$0
              @endproduction
              '''
  "@env('name') … @endenv ":
    'description': 'Determines whether application is running in the specified environment.'
    'descriptionMoreURL': 'https://laravel.com/docs/8.x/blade#environment-directives'
    'prefix': '@env'
    'body': '''
              @env('${1:name}')
              \t$0
              @endenv
              '''
